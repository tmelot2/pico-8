pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
cls()
x,y=0,0
t=0
p={8,9,10,11}

function _update()
	t+=1
	s=30
	for i=0,100 do
		x=60+s*cos(t/93)+s*sin(t/70)+5*tan(100)
		y=60+s*sin(t/100)+.5*s*sin(t/33)+1.5*5*sin(t/300)
		t+=1
		c=flr(sqrt(x^2+y^2))%16
		pset(x,y,c)
	end
end

function _draw()
  --cls()
end

function tan(a)
	return sin(a)/cos(a)
end
